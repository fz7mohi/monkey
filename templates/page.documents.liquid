<div class="image-grid">
  {% assign sorted_images = image_urls | sort %}
  {% for image_url in sorted_images %}
    {% assign image_name = image_url | split: '/' | last | split: '.' | first | downcase %}
    {% assign page_url = page_urls[forloop.index0] %}
    <div class="image-card" data-name="{{ image_name }}">
      <a href="{{ page_url }}">
        <img src="{{ image_url }}" alt="{{ image_name | capitalize }}" loading="lazy">
      </a>
    </div>
  {% endfor %}
</div>


<script>
  function filterStones() {
    var input = document.getElementById('searchInput');
    var filter = input.value.toLowerCase();
    var imageCards = document.getElementsByClassName('image-card');

    for (var i = 0; i < imageCards.length; i++) {
      var imageName = imageCards[i].getAttribute('data-name');
      if (imageName.includes(filter)) {
        imageCards[i].style.display = 'block';
      } else {
        imageCards[i].style.display = 'none';
      }
    }
  }
</script>